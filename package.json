{
  "name": "type-guardian",
  "version": "1.0.0",
  "description": "A recursive run-time type checking package that is simple to use yet provides all the functionality you will ever need. Written by and for Typescript.",
  "keywords": [
    "type",
    "check",
    "checker",
    "typescript",
    "runtime",
    "recursive",
    "guard",
    "guardian"
  ],
  "main": "js/index",
  "types": "dts/index",
  "files": [
    "js",
    "dts"
  ],
  "scripts": {
    "test": "jest",
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p tsconfig.json",
    "watch": "tsc -w -p .",
    "clean": "rm -rf js && rm -rf dts",
    "start": "node js/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elijahjcobb/type-guardian.git"
  },
  "author": "Elijah Cobb <elijah@elijahcobb.com> (https://elijahcobb.dev/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elijahjcobb/type-guardian/issues"
  },
  "homepage": "https://github.com/elijahjcobb/type-guardian#readme",
  "devDependencies": {
    "@types/jest": "latest",
    "@types/node": "latest",
    "jest": "latest",
    "typescript": "latest"
  }
}
